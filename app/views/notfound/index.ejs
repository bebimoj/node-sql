<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>404 Page Not Found</title>
    <link rel="icon" type="image/png" href="/favicon.png" sizes="72x72" />
    <%- include('../partials/styles') %>
  </head>
  <body>
    <style>
      .buttons{
        margin-top: 16px;
        text-align: center;
      }
      h1{
        font-weight: 500;
        line-height: 45px;
        margin-bottom: 30px;
      }
    </style>
    <div class="row vh flex flex-middle notfound">
    
      <div class="wrapper">
        <h1 class="text-center">The page you are looking for does not exist or has been removed.</h1>
        <div class="buttons">
          <a href="<%= ref %>" type="button" class="btn-border">
            <span class="text">Go Back To Where You Came From</span>
          </a>
        </div>
      </div>
    </div>
  </body>
</html>
